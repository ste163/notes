{
  "name": "notes",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build:ui:web": "tsc && vite build --mode web",
    "build:ui:desktop": "tsc && vite build --mode desktop",
    "start:dev:ui:web": "vite --mode web",
    "start:dev:ui:desktop": "vite --mode desktop",
    "start:dev:tauri": "tauri dev",
    "start:preview:ui": "pnpm run build:ui:web && vite preview --port 1420",
    "test": "vitest --watch=false",
    "test:watch": "vitest --watch=true",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "pre-commit": "tsc --noEmit && eslint .",
    "pre-push": "pnpm test",
    "prepare": "husky"
  },
  "dependencies": {
    "@tauri-apps/api": "1.6.0",
    "@tiptap/core": "2.6.6",
    "@tiptap/extension-bold": "2.6.6",
    "@tiptap/extension-bullet-list": "2.6.6",
    "@tiptap/extension-code-block": "2.6.6",
    "@tiptap/extension-document": "2.6.6",
    "@tiptap/extension-heading": "2.6.6",
    "@tiptap/extension-history": "2.6.6",
    "@tiptap/extension-italic": "2.6.6",
    "@tiptap/extension-link": "2.6.6",
    "@tiptap/extension-list-item": "2.6.6",
    "@tiptap/extension-ordered-list": "2.6.6",
    "@tiptap/extension-paragraph": "2.6.6",
    "@tiptap/extension-strike": "2.6.6",
    "@tiptap/extension-task-item": "2.6.6",
    "@tiptap/extension-task-list": "2.6.6",
    "@tiptap/extension-text": "2.6.6",
    "@tiptap/extension-underline": "2.6.6",
    "@tiptap/pm": "2.6.6",
    "events": "3.3.0",
    "nanoid": "5.0.7",
    "pouchdb-browser": "9.0.0",
    "pouchdb-find": "9.0.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.6.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "22.5.3",
    "@types/pouchdb-browser": "6.1.5",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "cypress": "13.14.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-testing-library": "6.3.0",
    "happy-dom": "15.7.3",
    "husky": "9.1.5",
    "prettier": "3.3.3",
    "typescript": "5.5.4",
    "vite": "5.4.3",
    "vite-plugin-markdown": "3.0.0-0",
    "vitest": "2.0.5"
  }
