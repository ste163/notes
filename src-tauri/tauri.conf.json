{
  "build": {
    "beforeDevCommand": "pnpm start:dev:ui:desktop",
    "beforeBuildCommand": "pnpm build:ui:desktop",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all",
    "macOS": {
      "signingIdentity": "-"
    }
  },
  "productName": "notes",
  "mainBinaryName": "notes",
  "version": "0.0.2",
  "identifier": "com.github.ste163.notes",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "notes",
        "width": 800,
        "height": 600,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}